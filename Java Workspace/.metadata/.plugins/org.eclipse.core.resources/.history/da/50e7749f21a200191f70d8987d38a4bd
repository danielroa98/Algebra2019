package org.danielRoa;

import java.util.*;

public class DivisionAndModulo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		double a = 10;
		double b = 3;
		
		double div = a/b;
		
		System.out.println(div);
		
		double mod = a/b;
		
		System.out.println(mod);
	}

}
