package first.api;

public class Calculator {

	private double a;
	private double b;
	
	public Calculator() {
		
	}
	
	public Calculator(double a, double b) {
		super();
		this.a = a;
		this.b = b;
	}
	
	public double add(){
		return a+b;
	}
	
	public double sub(){
		return a-b;
	}
	
	public double mul(){
		return a*b;
	}
	
	public double div(){
		return a/b;
	}
	
	
}
