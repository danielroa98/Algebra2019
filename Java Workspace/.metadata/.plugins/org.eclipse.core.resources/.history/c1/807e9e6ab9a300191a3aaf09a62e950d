package org.danielRoa;

public class StringExample {

	public static void main(String[] args) {

		String[] names = {"Peter", "Philip", "Michael", "Daniel"};
		
		for (String n : names) {
			
			n = n.toLowerCase();
			
			if (n.toLowerCase().startsWith("P")) {
				
				System.out.println(n);
				
			}
		
		}

	}

}
