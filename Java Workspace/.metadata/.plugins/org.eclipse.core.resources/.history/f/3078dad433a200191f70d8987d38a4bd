package org.danielRoa;

public class SwitchExample {
	public static void main(String[] args) {
		
		int grade = 4;
		
		switch (grade) {
		case 1:
			System.out.println("Failed");
			break;

		case 2:
			System.out.println("Not so good");
			break;
			
		case 3:
			System.out.println("Good");
			break;
		
		case 4:
			
			break;
		default:
			break;
		}
		
	}
}
