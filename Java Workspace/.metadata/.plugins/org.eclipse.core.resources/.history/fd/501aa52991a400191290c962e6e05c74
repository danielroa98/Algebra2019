		// TODO Auto-generated method stub
//		super.start();
		
		System.out.println("Sports car driving");
		
	}
	
	@Override
	public void stop() {
		// TODO Auto-generated method stub
//		super.stop();
	
		System.out.println("Sports car stopping");
	}
	
}