package org.danielRoa;

import java.util.*;

public class DivisionDouble {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		float a = 10;
		float b = 3;
		
		float div = a/b;
		
		System.out.println("The division is: "+div);
		
		double mod = a % b;
		
		System.out.println("The modulus is: "+mod);
	}

}
