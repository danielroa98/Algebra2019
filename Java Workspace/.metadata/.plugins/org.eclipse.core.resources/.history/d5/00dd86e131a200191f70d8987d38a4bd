public class IfElseExample {
	public static void main(String[] args) {
		
		int n = 24;
		
		if (n % 2 == 0) {
			System.out.println(" The number "+n+" It's an even");
		} else {
			System.out.println(" The number "+n+" It's an odd");
		}
		
	}
}
